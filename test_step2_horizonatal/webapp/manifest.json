{
    "_version": "1.49.0",
    "sap.app": {
        "id": "com.gsitm.fiori.teststep2.teststep2horizonatal", 
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
			"version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.9.3",
            "toolsId": "3f624a4b-ab01-40bb-9b0f-3f1dc09a577c"
        },
        "crossNavigation": {
            "inbounds": {
                "com-gsitm-fiori-teststep2-teststep2horizonatal-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Step2 Horizonatal Test",
                    "action": "DISPLAY",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.112.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.gsitm.fiori.teststep2.teststep2horizonatal.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                },
				{
					"uri": "RevealGrid/RevealGrid.css"
				}
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.gsitm.fiori.teststep2.teststep2horizonatal.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteStep2App",
                    "pattern": ":?query:",
                    "target": [
                        "TargetStep2App"
                    ]
                }
            ],
            "targets": {
                "TargetStep2App": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Step2App",
                    "viewName": "Step2App"
                }
            }
        },
        "rootView": {
            "viewName": "com.gsitm.fiori.teststep2.teststep2horizonatal.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
		"config": {
			"sample": {
				"files": [
					"V.view.xml",
					"C.controller.js",
					"manifest.json",
					"RevealGrid/RevealGrid.js",
					"RevealGrid/RevealGrid.css"
				]
			}
		}
    }
}